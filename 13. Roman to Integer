class Solution {
public:
    int romanToInt(string s) {
        int x = s.length();
            int ss[x];
            for(int i = 0; i < x; i++) {
                if (s[i] == 'I') {
                    ss[i] = 1;
                }
                else if (s[i] == 'V') {
                    ss[i] = 5;
                }
                else if (s[i] == 'X') {
                    ss[i] = 10;
                }
                else if (s[i] == 'L') {
                    ss[i] = 50;
                }
                else if (s[i] == 'C') {
                    ss[i] = 100;
                }
                else if (s[i] == 'D') {
                    ss[i] = 500;
                }
                else if (s[i] == 'M') {
                    ss[i] = 1000;
                }
            }
            int total = 0;
            for (int i = 0; i < x-1; i++) {
                if (ss[i] < ss[i+1]) {
                    total -= ss[i];
                }
                else {
                    total += ss[i];
                }
            }
            return total+=ss[x-1]; 
            }
};
